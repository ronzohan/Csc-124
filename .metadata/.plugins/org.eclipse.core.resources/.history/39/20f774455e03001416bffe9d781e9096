import java.awt.List;


public class RightDominant {

	public int[] findRightDominant(int[] numArray)
	{
		int key;
		int count=0; //count for right dominant
		boolean isRightDominant = false;
		int[] answerArray = new int[numArray.length]; 
		
		for(int i = 0;i<numArray.length;i++)
		{
			key = numArray[i];
			for(int j=i+1;j<numArray.length;j++)
			{
				
				if (key < numArray[j])
				{
					key = numArray[j];
					isRightDominant = true;
					for(int k=i+1;k<numArray.length;k++)
					{
						if (key < numArray[k])
							isRightDominant = false;
					}
					if (isRightDominant)
					{
						answerArray[count] = key;
						count++;
						key = numArray[j+1];
						i = j+1;
					}
				}
				
				
			}
			
			
		}
		
		
		
		return answerArray;
	}
}
